<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uelbosque.tic_tac_toe">
    <uses-permission android:name="android.permission.BLUETOOTH" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="TIC TAC TOE"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity

            android:name=".MainActivity">
            <intent-filter>

                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>


        <activity android:name=".Single.Single"/>

        <activity android:name=".Multiplayer.Multiplayer"/>

        <activity android:name=".Bluetooth.Bluetooth"/>

        <activity android:name=".Score.Score"/>


    </application>

</manifest>